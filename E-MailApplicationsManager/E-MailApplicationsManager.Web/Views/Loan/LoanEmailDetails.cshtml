@model StatusOpenEmailsViewModel

@{
    ViewData["Title"] = "LoanEmailDetails";
}

<script src="sweetalert2.all.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/promise-polyfill"></script>
<img src="~/images/tbilogo.png"  alt="Sample Photo" />
<body class="background">
    <div>
        <h4>StatusOpenEmailsViewModel</h4>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.GmailId)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.GmailId)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Name)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Name)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.EGN)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.EGN)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.PhoneNumber)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.PhoneNumber)
            </dd>
        </dl>
    </div>
    <div class="form-group row">
        <div class="col-xs-2">
            <button type="button" id="loan-approve" value="1 @Model.GmailId" style="background-color:#34495e" class="btn btn-primary">Approve</button>
            <button type="button" id="loan-reject" value="0 @Model.GmailId" style="background-color:#34495e" class="btn btn-primary">Reject</button>
        </div>
    </div>
    <div>
        <strong>
            <a class="goback" style="color:#ff6a00" href="javascript:history.go(-1)" title="Go to previous page">Go back</a>
        </strong>
    </div>
</body>